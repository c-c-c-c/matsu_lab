   "source": [
    "##画像の右と下に黒い部分を継ぎ足す\n",
    "\n",
    "def bulk_img( img, bulk_h, bulk_w):\n",
    "\n",
    "    h, w =  img.shape\n",
    "\n",
    "    dst = np.concatenate([img, np.zeros((bulk_h, w ))])\n",
    "    dst = np.concatenate([dst, np.zeros((h+bulk_h, bulk_w ))], axis = 1)\n",
    "\n",
    "    return (dst)\n"
   ]
  }
